{
  extends: ['airbnb', 'prettier'],
  parser: 'babel-eslint',
  env: {
    browser: true
  },
  rules: {
    'arrow-parens': 0,
    'import/prefer-default-export': 0,
    'max-len': [2, 80],
    'no-shadow': 0,
    'react/forbid-prop-types': 0,
    'react/jsx-filename-extension': 0,
    'react/no-danger': 0
  }
}
