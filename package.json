{
		"name": "sinclair",
		"description": "Personal website of David Sinclair",
		"author": "David Sinclair <david@sinclair.tech> (https://sinclair.tech)",
		"homepage": "sinclair.tech",
		"aws": "sinclairtech",
		"version": "0.0.1",
		"private": true,
		"scripts": {
				"dev": "node server.js",
				"build": "next build",
				"start": "NODE_ENV=production node server.js",
				"deploy": "serverless deploy",
				"destroy": "serverless remove",
				"eslint": "eslint \"./**/*.js\"",
				"export": "next build && next export -o build",
				"invalidate": "serverless invalidate",
				"next": "next",
				"prettier": "prettier --write \"./**/*.js\"",
				"server": "cd build && python -m SimpleHTTPServer",
				"sync": "serverless sync"
		},
		"dependencies": {
				"babel-eslint": "8.0.1",
				"babel-plugin-inline-import": "^2.0.6",
				"eslint": "4.11.0",
				"eslint-config-airbnb": "16.1.0",
				"eslint-config-prettier": "2.8.0",
				"eslint-plugin-import": "2.8.0",
				"eslint-plugin-jsx-a11y": "6.0.2",
				"eslint-plugin-react": "7.5.1",
				"express": "^4.16.2",
				"lodash": "4.17.4",
				"moment": "2.19.3",
				"next": "4.2.1",
				"next-redux-wrapper": "1.3.5",
				"node-sass": "4.7.2",
				"nprogress": "0.2.0",
				"path-match": "^1.2.4",
				"polished": "1.7.0",
				"prettier": "1.8.2",
				"prop-types": "15.6.0",
				"react": "16.2.0",
				"react-dom": "16.2.0",
				"react-redux": "5.0.6",
				"react-virtualized": "9.12.0",
				"react-window-size-listener": "1.0.10",
				"redux": "3.7.2",
				"redux-observable": "0.16.0",
				"redux-thunk": "2.2.0",
				"rxjs": "5.4.3",
				"serverless": "1.25.0",
				"showdown": "^1.8.6",
				"styled-jsx": "2.2.1",
				"styled-jsx-plugin-sass": "0.2.2"
		}
}
